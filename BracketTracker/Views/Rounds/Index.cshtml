@{
  Layout = "_Layout";
}

<h2>SHOW ME SOME ROUNDS, BOY</h2>

@using BracketTracker.Models
@* @model BracketTracker.Models.Round *@

@* @if (Model.Count == 0)
{
  <p>No rounds yet</p>
} *@

<ul>
@foreach (TeamRound join in Model.JoinEntities)
{
  if (join.Round.Result == false)
  {
    <li>@join.Team.Name</li>
  }
}
</ul>