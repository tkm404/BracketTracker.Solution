@{
  Layout = "_Layout";
}

@using BracketTracker.Models

@if (Model.Count == 0)
{
  <p>No teams yet</p>
}

<ul>
@foreach (Team team in Model)
{
  <li>@team.Name</li>
}
</ul>

@Html.ActionLink("Add another player", "Create")
<br>
@Html.ActionLink("That's enough players, I'm ready to check some boxes!", "Create", "Rounds")